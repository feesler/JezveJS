{
    "name": "jezvejs",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "module": "dist/js/index.js",
    "files": [
        "src",
        "tests"
    ],
    "description": "JavaScript utility and components library",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/feesler/JezveJS.git"
    },
    "bugs": {
        "url": "https://github.com/feesler/JezveJS/issues"
    },
    "homepage": "https://github.com/feesler/JezveJS#readme",
    "scripts": {
        "test": "node ./tests/index.js",
        "test-partial": "node ./tests/index.js -p",
        "all": "npm-run-all lint build test",
        "lint": "npm-run-all lint-src lint-style lint-demo-src lint-demo-style lint-tests",
        "lint-src": "eslint --quiet ./packages/jezvejs/src/**/*.js",
        "lint-tests": "eslint -c ./tests.eslintrc.json --quiet ./tests/ ./packages/jezvejs-test/**/*.js",
        "lint-style": "npx stylelint packages/jezvejs/src/**/*.scss",
        "lint-demo-src": "eslint --quiet ./demo/src/**/*.js",
        "lint-demo-style": "npx stylelint demo/src/**/*.scss",
        "debug": "node --inspect-brk ./tests/index.js",
        "build": "npm-run-all build-src build-local",
        "build-dev": "npm-run-all build-src-dev build-local-dev",
        "build-all": "npm-run-all build build-tests",
        "build-src": "npx webpack --progress -c ./config/common.js -c ./config/prod.js -m",
        "build-src-dev": "npx webpack --progress -c ./config/common.js -c ./config/dev.js -m",
        "build-demo": "npx webpack --progress -c ./config/demo.js -c ./config/prod.js -m",
        "build-local": "npx webpack --progress -c ./config/demo.js -c ./config/prod.js -c ./config/local.js -m",
        "build-demo-dev": "npx webpack --progress -c ./config/demo.js  -c ./config/dev.js -m",
        "build-local-dev": "npx webpack --progress -c ./config/demo.js  -c ./config/dev.js -c ./config/local.js -m",
        "build-tests": "npx webpack --progress -c ./config/tests.js",
        "serve": "npx webpack serve -c ./config/demo.js  -c ./config/dev.js -c ./config/local.js -m"
    },
    "author": "Henry Feesler",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.19.6",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-transform-regenerator": "^7.18.6",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.19.4",
        "astroturf": "^1.0.1",
        "autoprefixer": "^10.4.12",
        "babel-loader": "^8.2.5",
        "browserslist": "^4.21.4",
        "commonjs-variables-for-esmodules": "^1.1.0",
        "css-loader": "^5.2.7",
        "del-cli": "^3.0.1",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-import": "^2.26.0",
        "html-webpack-plugin": "^5.5.0",
        "jezve-test": "^1.0.0",
        "jezvejs-test": "^1.0.4",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1",
        "postcss-loader": "^5.3.0",
        "sass": "^1.55.0",
        "sass-loader": "^13.1.0",
        "style-loader": "^2.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^22.0.0",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.19.4",
        "core-js": "^3.26.0",
        "whatwg-fetch": "^3.6.2"
    }
}
